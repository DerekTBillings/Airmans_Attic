<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
	prefWidth="600.0" styleClass="blackAndWhiteTheme" stylesheets="@../stylesheets/CommonStylesheet.css"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.billings.controllers.AdminPageController">
	<children>
		<Label fx:id="adminHeaderLbl">
			<VBox.margin>
				<Insets bottom="15.0" top="15.0" />
			</VBox.margin>
		</Label>
		<TabPane prefHeight="339.0" prefWidth="580.0"
			tabClosingPolicy="UNAVAILABLE">
			<tabs>
				<Tab fx:id="infoTab">
					<content>
						<VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
							<children>
								<Label fx:id="atticInfoLbl">
									<VBox.margin>
										<Insets bottom="5.0" top="10.0" />
									</VBox.margin>
								</Label>
								<TextArea fx:id="atticInfo" prefHeight="63.0"
									prefWidth="580.0">
									<VBox.margin>
										<Insets left="10.0" right="10.0" />
									</VBox.margin>
								</TextArea>
								<Label fx:id="contactLbl">
									<VBox.margin>
										<Insets bottom="5.0" top="20.0" />
									</VBox.margin>
								</Label>
								<TableView fx:id="contactsTbl" prefHeight="157.0"
									prefWidth="580.0">
									<columns>
										<TableColumn fx:id="contactName" prefWidth="95.0" />
										<TableColumn fx:id="contactsCategory" prefWidth="85.0" />
										<TableColumn fx:id="contactsContactInfo"
											prefWidth="127.0" />
										<TableColumn fx:id="contactsComments" prefWidth="252.0" />
									</columns>
									<VBox.margin>
										<Insets left="10.0" right="10.0" />
									</VBox.margin>
								</TableView>
							</children>
						</VBox>
					</content>
				</Tab>
				<Tab fx:id="adminActionsTab">
					<content>
						<VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
							<children>
								<GridPane prefHeight="310.0" prefWidth="580.0">
									<columnConstraints>
										<ColumnConstraints hgrow="SOMETIMES"
											minWidth="10.0" prefWidth="100.0" />
										<ColumnConstraints hgrow="SOMETIMES"
											minWidth="10.0" prefWidth="100.0" />
									</columnConstraints>
									<rowConstraints>
										<RowConstraints minHeight="10.0" prefHeight="30.0"
											vgrow="SOMETIMES" />
										<RowConstraints minHeight="10.0" prefHeight="30.0"
											vgrow="SOMETIMES" />
										<RowConstraints minHeight="10.0" prefHeight="30.0"
											vgrow="SOMETIMES" />
									</rowConstraints>
									<children>
										<Button fx:id="raffleAdminBtn" mnemonicParsing="false"
											GridPane.halignment="CENTER" />
										<Button fx:id="customerLookupBtn" mnemonicParsing="false"
											GridPane.columnIndex="1" GridPane.halignment="CENTER" />
										<Button fx:id="generateReportsBtn" mnemonicParsing="false"
											GridPane.halignment="CENTER" GridPane.rowIndex="1" />
										<Button fx:id="addRemoveContactsBtn" mnemonicParsing="false"
											GridPane.columnIndex="1" GridPane.halignment="CENTER"
											GridPane.rowIndex="1" />
										<Button fx:id="addEventsBtn" mnemonicParsing="false"
											GridPane.halignment="CENTER" GridPane.rowIndex="2" />
										<Button fx:id="editAtticInfoBtn" mnemonicParsing="false"
											GridPane.columnIndex="1" GridPane.halignment="CENTER"
											GridPane.rowIndex="2" />
									</children>
								</GridPane>
							</children>
						</VBox>
					</content>
				</Tab>
				<Tab fx:id="notesTab">
					<content>
						<VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
							<children>
								<Label fx:id="atticNotesLbl">
									<VBox.margin>
										<Insets bottom="5.0" top="10.0" />
									</VBox.margin>
								</Label>
								<TextArea fx:id="atticNotes" prefHeight="74.0"
									prefWidth="580.0">
									<VBox.margin>
										<Insets left="10.0" right="10.0" />
									</VBox.margin>
								</TextArea>
								<Label fx:id="miscLbl">
									<VBox.margin>
										<Insets bottom="5.0" top="10.0" />
									</VBox.margin>
								</Label>
								<GridPane>
									<columnConstraints>
										<ColumnConstraints hgrow="SOMETIMES"
											maxWidth="168.0" minWidth="10.0" prefWidth="146.0" />
										<ColumnConstraints hgrow="SOMETIMES"
											maxWidth="161.0" minWidth="10.0" prefWidth="109.0" />
										<ColumnConstraints hgrow="SOMETIMES"
											maxWidth="111.0" minWidth="10.0" prefWidth="56.0" />
										<ColumnConstraints hgrow="SOMETIMES"
											maxWidth="160.0" minWidth="10.0" prefWidth="122.0" />
										<ColumnConstraints hgrow="SOMETIMES"
											maxWidth="143.0" minWidth="10.0" prefWidth="143.0" />
									</columnConstraints>
									<rowConstraints>
										<RowConstraints minHeight="10.0" prefHeight="30.0"
											vgrow="SOMETIMES" />
									</rowConstraints>
									<children>
										<Label fx:id="bagCountTextLbl" GridPane.halignment="RIGHT">
											<padding>
												<Insets right="5.0" />
											</padding>
										</Label>
										<Label fx:id="bagCountLbl" GridPane.columnIndex="1"
											GridPane.halignment="LEFT">
											<GridPane.margin>
												<Insets left="10.0" />
											</GridPane.margin>
										</Label>
										<Button fx:id="increaseBagCountBtn" mnemonicParsing="false"
											GridPane.columnIndex="1" GridPane.halignment="CENTER" />
										<CheckBox fx:id="animalDonationChk" mnemonicParsing="false"
											GridPane.columnIndex="4" />
										<CheckBox fx:id="churchCameChk" mnemonicParsing="false"
											GridPane.columnIndex="3" />
									</children>
								</GridPane>
								<Label fx:id="historyLbl">
									<VBox.margin>
										<Insets bottom="5.0" top="10.0" />
									</VBox.margin>
								</Label>
								<TableView fx:id="historyTbl" prefHeight="110.0"
									prefWidth="580.0">
									<columns>
										<TableColumn fx:id="historyBagCount" prefWidth="124.0" />
										<TableColumn fx:id="historyChurchCame" prefWidth="141.0" />
										<TableColumn fx:id="historyAnimalDonations"
											prefWidth="160.0" />
										<TableColumn fx:id="historyDate" prefWidth="134.0" />
									</columns>
									<VBox.margin>
										<Insets left="10.0" right="10.0" />
									</VBox.margin>
								</TableView>
							</children>
						</VBox>
					</content>
				</Tab>
			</tabs>
			<VBox.margin>
				<Insets left="10.0" right="10.0" />
			</VBox.margin>
		</TabPane>
	</children>
</VBox>
