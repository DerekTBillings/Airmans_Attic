<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="292.0"
	prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.billings.controllers.GenerateReportsPageController"
	stylesheets="@../stylesheets/CommonStylesheet.css"
	styleClass="blackAndWhiteTheme">
	<children>
		<Label fx:id="pageHeaderLbl" text="Generate Reports Page">
			<VBox.margin>
				<Insets bottom="20.0" top="20.0" />
			</VBox.margin>
		</Label>
		<TableView fx:id="reportTable" prefHeight="161.0" prefWidth="600.0">
			<columns>
				<TableColumn fx:id="reportName" prefWidth="217.0" text="Report Name" />
				<TableColumn fx:id="reportDescription" prefWidth="382.0"
					text="Report Description" />
			</columns>
			<VBox.margin>
				<Insets left="10.0" right="10.0" />
			</VBox.margin>
		</TableView>
		<HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
			<children>
				<Button fx:id="generateBtn" mnemonicParsing="false" text="Generate Report">
					<HBox.margin>
						<Insets right="10.0" />
					</HBox.margin>
				</Button>
				<Button fx:id="closeBtn" mnemonicParsing="false" text="Close">
					<HBox.margin>
						<Insets left="10.0" />
					</HBox.margin>
				</Button>
			</children>
		</HBox>
	</children>
</VBox>
